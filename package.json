{
  "name": "chess-club-api",
  "version": "1.0.0",
  "description": "server side for chess club app",
  "main": "dist/index.js",
  "scripts": {
    "docker:start": "docker-compose up --build -d && npm run migrate",
    "docker:infra": "docker-compose up --build -d chess-club-db && && npm run docker:health && npm run migrate",
    "docker:health": "node --experimental-modules scripts/health.mjs",
    "docker:stop": "docker-compose down",
    "migrate": "./scripts/flyway-migrate.sh",
    "health": "node ./scripts/health.js",
    "start": "tsc && nodemon --experimental-modules dist/index.js",
    "test": "cd acceptance && npm test",
    "verify": "npm run docker:start && npm run docker:health && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mallett002/chess-club-api.git"
  },
  "keywords": [
    "chess",
    "chess club"
  ],
  "author": "William Mallett",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/passport-jwt": "^3.0.6",
    "apollo-server-core": "^3.3.0",
    "apollo-server-express": "^3.3.0",
    "bcryptjs": "2.4.3",
    "chess.js": "^0.12.0",
    "config": "^3.3.6",
    "datasource-sql": "^1.5.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-subscriptions": "^1.2.1",
    "graphql-ws": "^5.5.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^3.0.0",
    "nodemon": "^2.0.7",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2"
  }
}
